<?xml version="1.0"?> 

<node name="mixF" author="Libero Spagnolini">
	<help>Mix two floats.</help>
	<input>
		<property name="F1" type="float" default="0"/>		
		<property name="F2" type="float" default="0"/>		
		<property name="factor" type="float" default="0.5"/>		
	</input>
	<output>
		<property name="outValue" type="float" />		
	</output>
	<code>
<![CDATA[
$(outValue) = mix(($(F1)), ($(F2)), ($(factor)));
]]>
	</code>		
</node>