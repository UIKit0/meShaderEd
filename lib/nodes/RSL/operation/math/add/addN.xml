<?xml version="1.0"?>

<node name="addN" author="DRL">
	<help>Add two normals.</help>
	<input>
		<property name="N1" type="normal" default="normal(0)"/>
		<property name="N2" type="normal" default="normal(0)"/>
	</input>
	<output>
		<property name="outN" type="normal"/>
	</output>
	<code>
<![CDATA[
 $(outN) = $(N1) + $(N2);
]]>
	</code>
</node>