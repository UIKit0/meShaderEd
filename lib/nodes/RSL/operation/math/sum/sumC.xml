<?xml version="1.0"?> 

<node name="sumC" author="Libero Spagnolini">
	<help>Sum two colors.</help>
	<input>
		<property name="inColor1" type="color"/>		
		<property name="inColor2" type="color"/>		
		<property name="factor" type="float"/>		
	</input>
	<output>
		<property name="outColor" type="color" />		
	</output>
	<code>
<![CDATA[
 $(outColor) = mix(($(inColor1)), ($(inColor2)), ($(factor)));
]]>
	</code>		
</node>
