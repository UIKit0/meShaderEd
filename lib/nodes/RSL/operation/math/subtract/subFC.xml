<?xml version="1.0"?>

<node name="subFC" author="DRL">
	<help>Subtract floats to color.</help>
	<input>
		<property name="F" type="float" default="0"/>
		<property name="C" type="color" default="color(0)"/>
	</input>
	<output>
		<property name="outC" type="color"/>
	</output>
	<code>
<![CDATA[
 $(outC) = color( $(F) ) - $(C);
]]>
	</code>
</node>