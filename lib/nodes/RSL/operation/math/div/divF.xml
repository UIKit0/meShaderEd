<?xml version="1.0"?> 

<node name="divF" author="DRL">
	<help>Divide two floats.</help>
	<input>
		<property name="in1" type="float" default="1"/>		
		<property name="in2" type="float" default="1"/>		
	</input>
	<output>
		<property name="out" type="float"/>		
	</output>
	<code>
<![CDATA[
$(out) = $(in1) / $(in2);
]]>
	</code>		
</node>
