<?xml version="1.0"?> 

<node name="divCF" author="mesh">
	<help>Divide color by color.</help>
	<input>
		<property name="inColor" type="color"/>		
		<property name="inFloat" type="float" default="1.0"/>		
	</input>
	<output>
		<property name="outColor" type="color" />		
	</output>
	<code>
<![CDATA[
 $(outColor) = $(inColor) / $(inFloat);
]]>
	</code>		
</node>
