<?xml version="1.0"?> 

<node name="divC" author="mesh">
	<help>Divide color by color.</help>
	<input>
		<property name="inColor1" type="color"/>		
		<property name="inColor2" type="color"/>		
	</input>
	<output>
		<property name="outColor" type="color" />		
	</output>
	<code>
<![CDATA[
 $(outColor) = $(inColor1) / $(inColor2);
]]>
	</code>		
</node>
