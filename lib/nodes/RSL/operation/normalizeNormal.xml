<?xml version="1.0"?>

<node name="normalizeNormal" author="mesh">
	<help>Normalize input Normal.</help>
	<input>
		<property name="input" type="normal" default="normal (0, 0, 0)" />
	</input>
	<output>
		<property name="NN" type="normal"/>
	</output>
	<code>
<![CDATA[
$(NN) = normalize( $(input) );
]]>
	</code>		
</node>
