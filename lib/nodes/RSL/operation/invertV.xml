<?xml version="1.0"?> 

<node name="invertV" author="mesh">
	<help>
	Invert float value
	</help>
	<input>
	  <property name="input" type="vector" default="vector(0,0,0)"/>	
	</input>
	<output>
		<property name="result" type="vector" />		
	</output>
	<code>
<![CDATA[
$(result) = -$(input);
]]>
	</code>	
</node>
