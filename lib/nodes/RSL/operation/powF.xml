<?xml version="1.0"?> 

<node name="powF" author="Libero Spagnolini">
	<help>Pow two floats.</help>
	<input>
		<property name="inValue1" type="float"/>		
		<property name="inValue2" type="float"/>		
	</input>
	<output>
		<property name="outValue" type="float" />		
	</output>
	<code>
<![CDATA[
$(outValue) = pow($(inValue1), $(inValue2));
]]>
	</code>			
</node>
