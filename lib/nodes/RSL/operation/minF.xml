<?xml version="1.0"?> 

<node name="minF" author="mesh">
	<help>Min of two floats.</help>
	<input>
		<property name="A" type="float"/>		
		<property name="B" type="float"/>		
	</input>
	<output>
		<property name="result" type="float" />		
	</output>
	<code>
<![CDATA[
$(result) = min( $(A), $(B) );
]]>
	</code>		
</node>
