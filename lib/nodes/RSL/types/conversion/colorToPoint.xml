<?xml version="1.0"?>

<node name="colorToPoint" type="rsl_code" author="DRL">
	<help>Color to point converter.</help>
	<input>
		<property name="inC" type="color" default="color(0)" />
	</input>
	<output>
		<property name="outP" type="point" />
	</output>
	<code><![CDATA[
$(outP) = point($(inC));
]]>
	</code>
</node>