<?xml version="1.0"?> 

<node name="vectorToNormal" type="rsl_code">
	<help>Vector to normal converter.</help>
	<input>
		<property name="input" type="vector" default="vector (0, 0, 0)" />
	</input>
	<output>
		<property name="output" type="normal"/>
	</output>
	<code>
	  <![CDATA[
	  $(output) = normal( $(input) );
	  ]]>
	</code>
</node>
