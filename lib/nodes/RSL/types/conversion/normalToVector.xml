<?xml version="1.0"?> 

<node name="normalToVector" type="rsl_code">
	<help>Normal to vector converter.</help>
	<input>
		<property name="input" type="normal" default="normal (0, 0, 0)" />
	</input>
	<output>
		<property name="output" type="vector"/>
	</output>
	<code>
	  <![CDATA[
	  $(output) = vector( $(input) );
	  ]]>
	</code>
</node>
