<?xml version="1.0"?> 

<node name="ShadingModel" type="rsl_code">
	<help>ShadingModel node</help>
	<input>
		<property name="Ci" type="color" default="color(0.0, 0.0, 0.0)" />
		<property name="Oi" type="color" default="color(1.0, 1.0, 1.0)" />
	</input>
	<output>
	  <property name="outCi" type="color" default="color(0.0, 0.0, 0.0)" />
		<property name="outOi" type="color" default="color(1.0, 1.0, 1.0)" />
	</output>
	<code><![CDATA[
	$(outCi) = $(Ci);
	$(outOi) = $(Oi);
	]]>
	</code>
</node>